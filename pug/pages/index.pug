extends  ../_skeleton

//- Includes
include ../components/_ui-pagination

block variables
  - var activeCategory = 'home'
  - var pageName = 'Home'

block page
  // Page Header
  include ../sections/_header-classic

  // Section About 
  section.section.section-lg.bg-default
    .container
      h3 Выполенные задания
      ul.list-ordered
        - for (var item in menu.projects.dropdown)
          li: a(href= menu.projects.dropdown[item].link)= menu.projects.dropdown[item].title

  // Page Footer
  include ../sections/_footer-classic
